{
  "name": "mysql2-async",
  "version": "1.1.2",
  "description": "A wrapper for mysql2 to add convenience, especially when developing with async/await and async iterables.",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib-esm/index.js"
    },
    "./db": {
      "require": "./lib/db.js",
      "import": "./lib-esm/db.js"
    }
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "db": ["./lib/db.d.ts"]
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf ./lib && tsc",
    "mocha": "mocha -r ts-node/register test/**/*.ts --exit",
    "test": "./test.sh"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.9.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^9.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "mysql2": "^2.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/mysql2-async.git"
  },
  "keywords": [
    "mysql",
    "txstate"
  ],
  "author": "Nick Wing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/txstate-etc/mysql2-async/issues"
  },
  "homepage": "https://github.com/txstate-etc/mysql2-async#readme",
  "files": [
    "lib/**/*",
    "lib-esm/**/*"
  ]
}
